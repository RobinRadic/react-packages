{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "esModuleInterop": true,
        "resolveJsonModule": false,
        "allowJs": false,
        "declaration": true
    },
    "include": [
        "lib"
    ],
    "exclude": [
        "node_modules"
    ],
    "compileOnSave": true
}
