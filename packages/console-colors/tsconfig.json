{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "target": "es6",
        "moduleResolution": "node",
        "sourceMap": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "declaration": true,
        "outDir": "./dist"
    },
    "include": [
        "src"
    ],
    "exclude": [
        "./node_modules",
        "./types",
        "./es",
        "./lib",
        "./*.ts",
        "*.js"
    ],
    "compileOnSave": true
}
